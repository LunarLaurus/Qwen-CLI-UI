{
  "name": "qwen-cli-ui",
  "version": "1.5.0",
  "description": "A stylish web-based UI for Qwen CLI",
  "type": "module",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "client": "vite --host",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:clean": "vite build --force",
    "preview": "vite preview",
    "start": "npm run build && npm run server",
    "start:dev": "npm run build:dev && npm run server",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "lint": "eslint src/ server/ --ext .js,.jsx --ignore-pattern node_modules --ignore-pattern dist",
    "lint:fix": "eslint src/ server/ --ext .js,.jsx --fix",
    "typecheck": "tsc --noEmit --jsx react --skipLibCheck",
    "clean": "rimraf dist node_modules/.vite",
    "rebuild": "npm run clean && npm install && npm run build",
    "build:win": "powershell -ExecutionPolicy Bypass -File scripts/build.ps1",
    "build:unix": "bash scripts/build.sh",
    "audit:wcag": "node scripts/audit-themes-ci.js",
    "audit:wcag:full": "node scripts/audit-themes.js"
  },
  "keywords": [
    "qwen",
    "ai",
    "code",
    "ui",
    "assistant"
  ],
  "author": "Qwen CLI UI Contributors",
  "license": "MIT",
  "dependencies": {
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-markdown": "^6.3.3",
    "@codemirror/lang-python": "^6.2.1",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@tailwindcss/typography": "^0.5.16",
    "@uiw/react-codemirror": "^4.23.13",
    "@xterm/addon-clipboard": "^0.1.0",
    "@xterm/addon-fit": "^0.9.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.2.0",
    "chokidar": "^4.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.515.0",
    "mime-types": "^3.0.1",
    "multer": "^2.0.1",
    "node-fetch": "^2.7.0",
    "node-pty": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.8.1",
    "tailwind-merge": "^3.3.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "postcss": "^8.4.32",
    "sharp": "^0.34.2",
    "tailwindcss": "^3.4.0",
    "vite": "^7.0.4"
  }
}
